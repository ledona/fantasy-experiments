{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Debug Tests",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "purpose": [
                "debug-test"
            ],
            "console": "integratedTerminal",
            "env": {
                "SQLALCHEMY_WARN_20": "1",
                "PYDEVD_DISABLE_FILE_VALIDATION": "1",
                "PYDEVD_WARN_SLOW_RESOLVE_TIMEOUT": "5"
            },
            "justMyCode": true
        },
        {
            "name": "model-pt train-test regresssor",
            "type": "debugpy",
            "request": "launch",
            "module": "lib.regressor",
            "env": {
                "PYDEVD_DISABLE_FILE_VALIDATION": "1",
            },
            "justMyCode": true,
            "cwd": "${workspaceFolder}/models",
            "args": [
                // "train", "2026.01/mlb.json", "*",
                // "train", "2026.01/nba.json", "*", 
                // "train", "2026.01/nfl.json", "*", 
                "train", "2026.01/nhl.json", "*", 
                // "retrain", "/fantasy-isync/fantasy-modeling/2024.04/pt/MLB-H-DK.dk_score.tpot-light.2024-04-01T020727.model",
                // "--orig_cfg_file", "/fantasy-experiments/models/2024.04/mlb.json",

                // "--info",
                // "--slack",

                "--exists_mode", "reuse",
                "--data_dir", "/fantasy-isync/fantasy-modeling/2026.01/data",
                "--dest_dir", "/fantasy-isync/fantasy-modeling/2026.01/models-mid",
                // "--dest_dir", "/fantasy-isync/fantasy-modeling/2026.01/models-best",
                "--data_limit", "250000",

                "--algo", "tpot",
                // "--algo", "tpot-light",
                "--max_iter_mins", "5",
                "--gens", "10",
                "--pop", "20",
                "--early_stop", "2",
                "--n_jobs", "4",
                "--max_train_mins", "15",
                // "--verbose", "4",

                // "--algo", "autogluon",
                // "--ag:preset", "medium",
                // "--ag:preset", "best_v150"
                // "--max_train_mins", "1",
                // "--verbose", "3",

                // "--algo", "nn",
                // "--batch_size", "256",
                // "--hidden_size", "64",
                // "--layers", "3",
                // "--lr", ".000001",
                // "--max_epochs", "20",
                // "--early_stop", "2",
                // "--checkpoint_dir", "/tmp/fantasy-test-checks",
                // "--checkpoint_freq", "10"
                // "--nn:resume", "/tmp/fantasy-test-checks/nn-checkpoint-100.pt",
            ]
        },
        {
            "name": "model-pt catalog",
            "type": "debugpy",
            "request": "launch",
            "module": "lib.regressor",
            "justMyCode": true,
            "cwd": "${workspaceFolder}/models",
            "args": [
                "catalog",
                "--best",
                "--root", "/fantasy-isync/fantasy-modeling/2026.01"
                // "--exclude", ".*2023.12.*"
            ]
        },
        {
            "name": "model-pt performance",
            "type": "debugpy",
            "request": "launch",
            "module": "lib.regressor",
            "justMyCode": true,
            "cwd": "${workspaceFolder}/models",
            "args": [
                "performance",
                "/fantasy-isync/fantasy-modeling/2026.01/models-test/NBA-player-dreb.d_reb.autogluon.2026-01-28T171935.model",
                "--cfg", "/fantasy-experiments/models/2026.01/nba.json",
                "--op", "calc", // "--skip_backups",
                "--data_dir", "/fantasy-isync/fantasy-modeling/2026.01/data",
                // "--max_missing_infer", "1"
            ]
        },
        {
            "name": "game-predict - data",
            "type": "debugpy",
            "request": "launch",
            "module": "lib.all_game_pt",
            "justMyCode": true,
            "cwd": "${workspaceFolder}/models",
            "args": [
                "2024.04/mlb-game.json", "MLB-GAME-stats", "data",
                "/fantasy-isync/fantasy-modeling/2024.04/data",
                "/fantasy-isync/fantasy-modeling/2024.04/game"
            ]
        },
        {
            "name": "df-hist retrieval (win)",
            "type": "debugpy",
            "request": "launch",
            "module": "lib.retrieve.retrieve_hist",
            "justMyCode": true,
            "cwd": "${workspaceFolder}/df-hist",
            "args": [
                // "yahoo",
                "draftkings", 
                // "fanduel". 
                "--address", "127.0.0.1:9222",
                "--web-limit", "25",
                "--history-file-dir", "${env:FANTASY_IDRIVE_HOME}\\fantasy-dfs-hist\\betting",
                "--cache", "${env:FANTASY_IDRIVE_HOME}\\fantasy-dfs-hist\\betting\\df-hist-cache",
                "--skip-entry-filepath", "${workspaceFolder}\\df-hist\\skip_entries.txt",
                // "--cache-only", 
                "-o", "${env:FANTASY_IDRIVE_HOME}\\fantasy-dfs-hist\\betting",
                "--sports", "nba", 
                "--start-date", "20250418", // "--end-date", "2025-04-18",
            ]
        },
        {
            "name": "df-hist retrieval (linux-cache-only)",
            "type": "debugpy",
            "request": "launch",
            "module": "lib.retrieve.retrieve_hist",
            "justMyCode": true,
            "cwd": "${workspaceFolder}/df-hist",
            "args": [
                // "yahoo",
                "draftkings", 
                // "fanduel". 
                // "--address", "127.0.0.1:9222",
                // "--web-limit", "25",
                "--history-file-dir", "/fantasy-isync/fantasy-dfs-hist/betting",
                "--cache", "/fantasy-isync/fantasy-dfs-hist/betting/df-hist-cache",

                "--cache-only", 
                "-o", "/fantasy-isync/fantasy-dfs-hist/betting",
                // "--sports", "mlb", 
                // "--start-date", "20240901", "--end-date", "2025-04-18",
            ]
        },
        {
            "name": "df-hist xform",
            "type": "debugpy",
            "request": "launch",
            "module": "lib.xform.cli",
            "justMyCode": true,
            "cwd": "${workspaceFolder}/df-hist",
            "args": [
                "--cache_dir", "/fantasy-isync/fantasy-dfs-hist/data/xform-cache",
                // "--services", "draftkings",
                // "--style", "classic",
                // "--type", "GPP",
                // "--", "--dates", "20200901", "20201101",
                "--sports", "nfl", "nhl", "nba", "mlb"
            ]
        },
        {
            "name": "df-hist modeling",
            "type": "debugpy",
            "request": "launch",
            "module": "lib.modeling.cli",
            "justMyCode": true,
            "cwd": "${workspaceFolder}/df-hist",
            "args": [
                "--mode", "reuse", 
                "nfl", "mlb", "nhl", "nba", //"lol",
                "--model_path", "/fantasy-isync/fantasy-dfs-hist/models/2025.07",
                "--results_path", "/fantasy-isync/fantasy-dfs-hist/eval_results/2025.07",
                "--data_path", "/fantasy-isync/fantasy-dfs-hist/data",
                "--framework", "reg_chain", "tpot"
            ]
        },
        {
            "name": "venue features",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/stat-analysis/venue_features.py",
            "justMyCode": true,
            "cwd": "${workspaceFolder}/stat-analysis",
            "args": [
                // "/fantasy/mlb.hist.2008-2025.scored.db",
                "/fantasy/nfl.hist.2009-2024.scored.db",
                // "/fantasy/nba.hist.20082009-20242025.scored.db",
                // "/fantasy/nhl.hist.20082009-20242025.scored.db",
                "pf",
                // "thfa",
                // "--min_season", "20222023",
                // "--max_season", "20242025",

                // "--parquet", // "/fantasy-archive/mlb/park_factor-2026.parquet",
                "--csv", // "/fantasy-archive/mlb/park_factor.2008-2026.csv"
            ]
        }
    ]
}