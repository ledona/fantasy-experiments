    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Debug Tests",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "purpose": [
                "debug-test"
            ],
            "console": "integratedTerminal",
            "env": {
                "SQLALCHEMY_WARN_20": "1",
                "PYDEVD_DISABLE_FILE_VALIDATION": "1"
            },
            "justMyCode": true
        },
        {
            "name": "train_test CLI",
            "type": "python",
            "request": "launch",
            "module": "lib.cli",
            "justMyCode": true,
            "cwd": "${workspaceFolder}/models",
            "args": [
                // "--info",
                "--tpot_jobs", "2",
                // "--time", "2",
                "--automl", "dummy",
                "--dest", "/tmp",
                "2023.12/nhl.json",
                // "--info",
                "NHL-goalie-DK"
            ]
        },
        {
            "name": "Y Real Full (web-limit 10 + existing browser + no-interactive)",
            "type": "python",
            "request": "launch",
            "program": "retrieve_hist.py",
            "console": "integratedTerminal",
            "args": [
                "yahoo",
                // "draftkings", 
                // "fanduel". 
                "--address",
                "127.0.0.1:9222",
                "--web-limit",
                "10",
                "--cache",
                "cache",
                // "--end-date", "2018-10-27",
            ]
        },
    ]
}