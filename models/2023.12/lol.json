{
    "global_default": {
        "seed": 1,
        "validation_season": 2022,
        "recent_games": 3,
        "missing_data_threshold": 0.15,
        "train_params": {
            "n_jobs": 3
        }
    },
    "model_groups": [
        // team models
        {
            "data_filename": "lol_team.pq",
            "p_or_t": "T",
            "train_params": {
                "max_time_mins": 10
            },
            "models": {
                "LOL-team-win": {
                    "training_seasons": [
                        2014,
                        2015,
                        2016,
                        2017,
                        2018,
                        2019,
                        2020,
                        2021
                    ],
                    "target": [
                        "extra",
                        "match_win"
                    ]
                },
                "LOL-team-DK": {
                    "training_seasons": [
                        2016,
                        2017,
                        2018,
                        2019,
                        2020,
                        2021
                    ],
                    "target": [
                        "calc",
                        "dk_performance_score"
                    ],
                    "cols_to_drop": [
                        "extra:match_win"
                    ]
                }
            }
        },
        // player models
        {
            "data_filename": "lol_player.pq",
            "p_or_t": "P",
            "include_pos": true,
            "train_params": {
                "max_time_mins": 30
            },
            "training_seasons": [
                2016,
                2017,
                2018,
                2019,
                2020,
                2021
            ],
            "models": {
                "LOL-player-DK": {
                    "target": [
                        "calc",
                        "dk_performance_score"
                    ]
                }
            }
        }
    ]
}